CREATE TABLE IF NOT EXISTS ProdCalendar (
	date	INTEGER NOT NULL UNIQUE,
	type_id	INTEGER NOT NULL DEFAULT 1,
	type_text	TEXT,
	note	TEXT,
	week_day	TEXT,
	working_hours	INTEGER DEFAULT 0
);

CREATE INDEX IF NOT EXISTS prod_calendar_date ON ProdCalendar (
	date	ASC
);

selectAll:
SELECT *
FROM ProdCalendar;

insert:
REPLACE INTO ProdCalendar
    VALUES ?;